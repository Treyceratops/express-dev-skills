<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Cats</title>
</head>

<body>
    <h1>Cats as of <%= time %>
    </h1>

    <ul>
        <% cats.forEach(function(c) { %>
            <li>
                <a href="/cats/<%= c.id %>">
                    <%= c.cat %>
                </a>
                <%= c.cat %>
                    -
                    <%= c.cute ? 'cute' : 'not cute' %>
            </li>
            <% }); %>
    </ul>
</body>

</html>